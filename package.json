{
  "name": "next-web-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build:test": "vite build --mode test",
    "build:prod": "tsc && vite build --mode production",
    "serve": "vite preview",
    "lint": "tsc --noEmit && eslint src --ext .ts,.tsx",
    "prettier": "prettier --write 'src/**/*.{js,json,yml,yaml,ts,tsx,md}'",
    "ncu": "npx npm-check-updates -u -x zustand,husky",
    "prepare": "husky install",
    "lint-staged": "lint-staged --allow-empty"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@loadable/component": "^5.15.0",
    "ahooks": "^2.10.6",
    "antd": "^4.16.3",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.17.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "wbd-frontend-kit": "^1.0.13",
    "zustand": "3.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@fruits-chain/eslint-config-preset": "^0.0.3",
    "@types/loadable__component": "^5.13.3",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "husky": "6.0.0",
    "less": "^4.1.1",
    "lint-staged": "^11.0.0",
    "pont-engine": "1.0.13",
    "postcss": "^8.3.5",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "vite-plugin-style-import": "^1.0.0"
  },
  "lint-staged": {
    "*.{yml,yaml,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write",
      "git add"
    ]
  }
}
