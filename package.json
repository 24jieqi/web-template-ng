{
  "name": "orange-admin-web",
  "private": true,
  "version": "0.1.0",
  "appName": "XX管理系统",
  "scripts": {
    "dev": "vite --mode dev",
    "build:dev": "tsc && vite build --mode dev",
    "build:test": "tsc && vite build --mode test",
    "build:demo": "tsc && vite build --mode staging",
    "build:prod": "tsc && vite build --mode production",
    "lint": "node_modules/.bin/eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint:ts": "tsc --noEmit && node_modules/.bin/eslint src --ext .ts,.tsx",
    "lint:style": "stylelint 'src/**/*.less'",
    "lint-staged": "lint-staged --allow-empty",
    "commit": "npx git-cz",
    "gen": "fruits-chain-gc https://java-orange-graphql-dev.k8s-dev.hjfruit.cn/graphql",
    "gen:w": "fruits-chain-gc https://java-orange-graphql-dev.k8s-dev.hjfruit.cn/graphql -w",
    "test": "vitest"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@apollo/client": "^3.7.14",
    "@fruits-chain/design-tokens-bailu": "^0.0.7",
    "@fruits-chain/icons-react": "^0.0.31",
    "@fruits-chain/react-bailu": "^0.3.0-beta.3",
    "@fruits-chain/utils": "^1.0.1",
    "@types/lodash": "^4.14.196",
    "ahooks": "^3.7.8",
    "antd": "^5.11.1",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "graphql": "^16.6.0",
    "kiwi-intl": "^1.2.6-beta.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "sm-crypto": "^0.3.13",
    "web-hw-obs": "^1.2.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@fruits-chain/eslint-config-preset": "^0.2.21",
    "@fruits-chain/graphql-codegen-preset": "^0.3.1",
    "@fruits-chain/prettier-config-preset": "^0.0.3",
    "@fruits-chain/stylelint-config-preset": "^0.0.9",
    "@graphql-codegen/cli": "3.2.2",
    "@hjfc/playwright-utils": "^1.0.1",
    "@playwright/test": "^1.39.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.8.8",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/sm-crypto": "^0",
    "@vitejs/plugin-react": "^4.0.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "mockjs": "^1.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-top-level-await": "^1.3.1",
    "vitest": "^0.31.2"
  },
  "prettier": "@fruits-chain/prettier-config-preset",
  "lint-staged": {
    "*.{yml,yaml,md}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "bash -c \"tsc -p ./tsconfig.json --noEmit\"",
      "eslint --fix",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "yarn@4.0.2"
}
