{
  "name": "next-web-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --mode dev",
    "build:dev": "vite build --mode dev",
    "build:test": "vite build --mode test",
    "build:prod": "tsc && vite build --mode prod",
    "serve": "vite preview --host",
    "lint": "tsc --noEmit && eslint src --ext .ts,.tsx",
    "prettier": "prettier --write 'src/**/*.{js,json,yml,yaml,ts,tsx,md}'",
    "ncu": "npx npm-check-updates -u -x zustand,husky",
    "prepare": "husky install",
    "resolutions": "npx npm-force-resolutions",
    "lint-staged": "lint-staged --allow-empty"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@apollo/client": "^3.3.21",
    "@loadable/component": "^5.15.0",
    "ahooks": "^2.10.6",
    "antd": "^4.16.3",
    "axios": "^0.21.1",
    "graphql": "^15.5.1",
    "lodash": "^4.17.21",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "wbd-frontend-kit": "^1.0.13",
    "zustand": "3.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@fruits-chain/eslint-config-preset": "^0.0.3",
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/near-operation-file-preset": "^1.18.4",
    "@graphql-codegen/schema-ast": "^1.18.3",
    "@graphql-codegen/typescript": "^1.22.4",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@graphql-codegen/typescript-react-apollo": "^2.3.0",
    "@types/loadable__component": "^5.13.3",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "husky": "6.0.0",
    "less": "^4.1.1",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.5",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "vite-plugin-style-import": "^1.0.0"
  },
  "resolutions": {
    "prettier": "^2.3.1"
  },
  "lint-staged": {
    "*.{yml,yaml,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "bash -c tsc --noEmit",
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
